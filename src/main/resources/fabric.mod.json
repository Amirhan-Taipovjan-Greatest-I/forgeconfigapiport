{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",

  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthor}",
    "LexManos",
    "cpw",
    "Anyone who has contributed to the Forge project on Github and supports their development"
  ],
  "contact": {
    "homepage": "${modPageUrl}",
    "issues": "${modIssueUrl}",
    "sources": "${modSourceUrl}"
  },

  "license": "GNU Lesser General Public License 2.1",
  "icon": "mod_logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${mainEntryPoint}"
    ],
    "client": [
      "${clientEntryPoint}"
    ]
  },
  "mixins": [
    "${modId}.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${minFabricVersion}",
    "fabric": ">=${minFabricApiVersion}",
    "minecraft": ">=${minMinecraftVersion} <${nextMinecraftVersion}",
    "java": ">=16"
  }
}
